{
  "name": "string-utils",
  "version": "0.0.1",
  "description": "String manipulation and processing functions",
  "main": "src/index.js",
  "type": "module",
  "homepage": "https://nyxlang.github.io/string-utils/",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "rm -fr dist/* && npm run build:packages && npm run build:browser",
    "build:packages": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && node ./scripts/set-package-json.js",
    "build:browser": "parcel build src/index.js --out-file string-utils.js --no-cache",
    "test": "jest",
    "test:all": "jest --all",
    "test:watch": "jest --watch",
    "test:changed": "jest --onlyChanged",
    "docs": "rm -rf docs/* && jsdoc -c .jsdoc.json",
    "prepare": "husky install",
    "format": "prettier --write . && eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyxlang/string-utils.git"
  },
  "keywords": [
    "string",
    "utilities"
  ],
  "author": {
    "name": "Jason Barr",
    "email": "jason@jasonsbarr.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nyxlang/string-utils/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-env": "^7.13.15",
    "babel-jest": "^26.6.3",
    "eslint": "^7.24.0",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "lint-staged": "^10.5.4",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "unicode-default-word-boundary": "^13.0.0"
  }
}
