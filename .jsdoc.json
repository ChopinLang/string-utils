{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src/", "README.md"]
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "default": {
      "outputSourceFiles": false
    }
  }
}
